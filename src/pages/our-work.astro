---
import Layout from '../layouts/Layout.astro';
import Header from '../components/Header.astro';
import Footer from '../components/Footer.astro';
import { 
  Rocket, 
  Users, 
  GraduationCap, 
  BookOpen, 
  Heart,
  ArrowRight,
  ExternalLink,
  Code,
  Brain,
  Shield,
  Zap,
  Globe,
  Award,
  Target,
  TrendingUp,
  Building,
  Lightbulb,
  CheckCircle,
  Scale,
  FileText,
  MapPin,
  Clock,
  DollarSign,
  Briefcase,
  Database,
  Eye,
  Lock,
  MessageCircle
} from 'lucide-react';
---

<Layout 
  title="$23M-$40M AI Platform Portfolio | AutiMind Technical Excellence"
  description="Discover AutiMind's $23M-$40M AI platform portfolio. Our production platforms include AstroLMS (AI learning disability detection, $25-50M value), ZServed (legal automation + DAO governance, $75-125M value), and EtchNFT (phygital NFT marketplace, $8-12M value). Proven technical capabilities in AI, blockchain, and accessibility solutions."
  keywords="AutiMind portfolio valuation, AstroLMS learning disability detection, ZServed legal automation value, EtchNFT physical NFT, multi-million AI portfolio, AI platform development, legal document processing AI, advanced AI capabilities, blockchain AI systems, enterprise AI development"
  canonicalUrl="https://autimind.com/our-work"
>
  <Header />
  
  <!-- Hero Section -->
  <section class="pt-32 pb-20 bg-gradient-to-br from-gray-900 via-gray-800 to-gray-900">
    <div class="container mx-auto px-4">
      <div class="text-center max-w-6xl mx-auto">
        <div class="inline-flex items-center gap-2 px-4 py-2 rounded-full bg-gradient-to-r from-green-500/20 to-emerald-500/20 border border-green-500/30 mb-8">
